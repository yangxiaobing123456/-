#import <Foundation/Foundation.h>

@interface YellowPageInfo : NSObject

@property long long noticeId;
@property (nonatomic, strong) NSString *strId;
@property long long cityId;
@property long long communityId;
@property long long updateTime;
@property long long createTime;
@property (nonatomic, strong) NSString *name;
@property (nonatomic, strong) NSString *content;
@property (nonatomic, strong) NSString *content2;
@property int type;
@property int pageType;

//当type=1,3,4,5的时候有下面4个属性
@property double lon;
@property double lat;
@property (nonatomic, strong) NSString *address;
@property (nonatomic, strong) NSString *telephone;

//当type=2的时候有下面2个属性
@property (nonatomic, strong) NSString *serviceTime;
@property (nonatomic, strong) NSArray *stations;
@property (nonatomic, strong) NSString *station;    //表示当前小区当前站名

@end